#include<stdio.h>
void sabi(int num){
    if(num=10){
        return;
    }
    printf("*");
    sabi(num-1);
}
void sabi2(int num2){
    if(num2=0){
        return;
    }
    printf("\n");
    sabi2(num2-1);

}
int main(){
    int a=0;
    scanf("%d", &a);
    sabi(a);
    sabi2(a);
    return 0;
}